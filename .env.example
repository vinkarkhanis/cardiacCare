# Azure Cosmos DB Configuration
# Copy this file to .env.local and fill in your actual values

COSMOS_DB_ENDPOINT=your_cosmos_db_endpoint_here
COSMOS_DB_KEY=your_cosmos_db_key_here
COSMOS_DB_DATABASE_NAME=CardiacHealthDB
COSMOS_DB_CONTAINER_NAME=patients

# Azure AI Foundry - Cardiac Agent Configuration
AZURE_AI_FOUNDRY_PROJECT_ENDPOINT=your_foundry_project_endpoint_here
AZURE_AI_FOUNDRY_API_KEY=your_foundry_api_key_here
OPENAI_API_VERSION=2024-12-01-preview

# Azure AI Foundry Agent IDs (Replace with your actual agent IDs)
AZURE_AI_ORCHESTRATION_AGENT_ID=your_orchestration_agent_id_here
AZURE_AI_NURSING_AGENT_ID=your_nursing_agent_id_here
AZURE_AI_EXERCISE_AGENT_ID=your_exercise_agent_id_here
AZURE_AI_DIET_AGENT_ID=your_diet_agent_id_here
AZURE_AI_MEDICATION_AGENT_ID=your_medication_agent_id_here

# Azure Subscription Information
AZURE_SUBSCRIPTION_ID=your_subscription_id_here
AZURE_RESOURCE_GROUP_NAME=your_resource_group_name_here
AZURE_PROJECT_NAME=your_project_name_here

# Application Environment
NODE_ENV=development

# Next.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# Example:
# AZURE_AI_FOUNDRY_PROJECT_ENDPOINT=https://your-resource.services.ai.azure.com/api/projects/your-project
# AZURE_AI_FOUNDRY_API_KEY=your-foundry-api-key
# AZURE_AI_ORCHESTRATION_AGENT_ID=asst_xxxxxxxxxxxxxxxxx